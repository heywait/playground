{
    "name": "munchkin",
    "description": "Web version of table game Munchkin",
    "author": "heywait",
    "version": "0.0.1",
    "scripts": {
        "build": "better-npm-run build-dev",
        "server": "better-npm-run server-dev"
    },
    "betterScripts": {
        "build-dev": {
            "command": "node ./node_modules/webpack/bin/webpack.js --config ./webpack.config.js --progress --colors --watch",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "server-dev": {
            "command": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors"
        }
    },
    "dependencies": {
        "react": "15.3.0",
        "react-dom": "15.3.0",
        "typescript": "1.8.10"
    },
    "devDependencies": {
        "webpack": "1.13.1",
        "webpack-dev-server": "1.14.1",
        "typescript": "1.8.10",
        "better-npm-run": "0.0.10",
        "babel-core": "6.11.4",
        "babel-loader": "6.2.4",
        "babel-preset-react": "6.11.1",
        "babel-preset-es2015": "6.9.0",
        "babel-preset-stage-0": "6.5.0",
        "react-hot-loader": "1.3.0",
        "typings": "1.3.2"
    }
}